{
  "name": "prepaid-list-backend",
  "version": "1.0.0",
  "description": "Backend for the FSIM OTH-Regensburg prepaid list",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run swagger && npm run transpile && npm run tslint",
    "tslint": "./node_modules/.bin/tslint --project .",
    "swagger": "swaggerGen -c ./swaggerConfig.json && npm run copyFiles",
    "copyFiles": "copyfiles -f src/doc/swagger* dist/doc",
    "transpile": "tsc",
    "start": "node ./dist/index.js",
    "debug": "tsc && set NODE_ENV=dev && node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.3",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^8.3.0",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "minio": "^7.0.5",
    "mongoose": "^5.4.10",
    "mongoose-autopopulate": "^0.9.1",
    "mongoose-history": "^0.8.0",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "string-template": "^1.0.0",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.1",
    "@types/express-validator": "^3.0.0",
    "@types/i18n": "^0.8.3",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lusca": "^1.5.0",
    "@types/method-override": "0.0.31",
    "@types/minio": "^7.0.1",
    "@types/mongoose": "^5.3.12",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^0.4.7",
    "@types/passport-local": "^1.0.33",
    "@types/string-template": "^1.0.2",
    "@types/swagger-ui-express": "^3.0.0",
    "copyfiles": "^2.1.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "typescript-rest-swagger": "0.0.22"
  }
}
